{
    "name": "kyleJennings/bootswatch",
    "description": "WordPress theme built with _s, Bootstrap, and the WP Customizer",
    "type": "project",
    "license": "MIT",
    "repositories": [
    ],
    "require": {
      "twbs/bootstrap-sass" : "3.3.x-dev",
      "leafo/scssphp": "0.6.6",
      "thomaspark/bootswatch": "3.3.7",
      "fortawesome/font-awesome" : "4.7.0"
    },
    "autoload": {
        "psr-4": {
            "bootswatch\\": "bootswatch/"
        }
    },
    "scripts" : {
        "post-install-cmd": "bootswatch\\BuildBootswatches::buildCSS",
        "build-css": "bootswatch\\BuildBootswatches::buildCSS"
    }
}
